void testSerpent (void)
{
#ifndef __SDCC_ds390 // Bug #3908.
#ifndef LACK_OF_MEMORY
  serpent_subkeys_t subkeys;
  unsigned char scratch[16];
  unsigned int i;

  for (i = 0; test_data[i].key_length; i++)
    {
      serpent_setkey_internal (subkeys, test_data[i].key,
                               test_data[i].key_length);
#if defined (OP_encrypt)
      serpent_encrypt_internal (subkeys,
				(const uint32_t *) test_data[i].text_plain,
				(uint32_t *) scratch);
      ASSERT (!memcmp (scratch, test_data[i].text_cipher, sizeof (serpent_block_t)));
#endif

#if defined (OP_decrypt)
      serpent_decrypt_internal (subkeys,
			      (const uint32_t *) test_data[i].text_cipher,
			      (uint32_t *) scratch);
      ASSERT (!memcmp (scratch, test_data[i].text_plain, sizeof (serpent_block_t)));
#endif
    }
#endif
#endif
}

