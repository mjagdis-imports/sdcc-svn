/* XOR BLOCK1 into BLOCK0.  */
#define BLOCK_XOR(block0, block1) \
  {                               \
    block0[0] ^= block1[0];       \
    block0[1] ^= block1[1];       \
    block0[2] ^= block1[2];       \
    block0[3] ^= block1[3];       \
  }

#define SBOX_IMPL(which, array) \
  {uint32_t a = array[index + 0]; uint32_t b = array[index + 1]; uint32_t c = array[index + 2]; uint32_t d = array[index + 3]; SBOX##which(a, b, c, d, array[index + 0], array[index + 1], array[index + 2], array[index + 3]);}

static void sbox0(uint32_t *array, uint_fast8_t index) SBOX_IMPL(0, array)
static void sbox1(uint32_t *array, uint_fast8_t index) SBOX_IMPL(1, array)
