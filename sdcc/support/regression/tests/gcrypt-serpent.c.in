/* Apply the linear transformation to BLOCK.  */
#define LINEAR_TRANSFORMATION(block)                  \
  {                                                   \
    block[0] = stdc_rotate_left (block[0], 13);       \
    block[2] = stdc_rotate_left (block[2], 3);        \
    block[1] = block[1] ^ block[0] ^ block[2];        \
    block[3] = block[3] ^ block[2] ^ (block[0] << 3); \
    block[1] = stdc_rotate_left (block[1], 1);        \
    block[3] = stdc_rotate_left (block[3], 7);        \
    block[0] = block[0] ^ block[1] ^ block[3];        \
    block[2] = block[2] ^ block[3] ^ (block[1] << 7); \
    block[0] = stdc_rotate_left (block[0], 5);        \
    block[2] = stdc_rotate_left (block[2], 22);       \
  }

/* Apply the inverse linear transformation to BLOCK.  */
#define LINEAR_TRANSFORMATION_INVERSE(block)          \
  {                                                   \
    block[2] = stdc_rotate_right (block[2], 22);      \
    block[0] = stdc_rotate_right (block[0] , 5);      \
    block[2] = block[2] ^ block[3] ^ (block[1] << 7); \
    block[0] = block[0] ^ block[1] ^ block[3];        \
    block[3] = stdc_rotate_right (block[3], 7);       \
    block[1] = stdc_rotate_right (block[1], 1);       \
    block[3] = block[3] ^ block[2] ^ (block[0] << 3); \
    block[1] = block[1] ^ block[0] ^ block[2];        \
    block[2] = stdc_rotate_right (block[2], 3);       \
    block[0] = stdc_rotate_right (block[0], 13);      \
  }

/* Apply a Serpent round to BLOCK, using the SBOX number WHICH and the
   subkeys contained in SUBKEYS.  Use BLOCK_TMP as temporary storage.
   This macro increments `round'.  */
#define ROUND_IMPL(which, block, subkeys, round) \
  {                                              \
    BLOCK_XOR (block, subkeys[round]);           \
    sbox##which(block, 0);                       \
    LINEAR_TRANSFORMATION (block);               \
  }

/* Apply the last Serpent round to BLOCK, using the SBOX number WHICH
   and the subkeys contained in SUBKEYS.  Use BLOCK_TMP as temporary
   storage.  The result will be stored in BLOCK_TMP.  This macro
   increments `round'.  */
static void last_round(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round)
{
  BLOCK_XOR(block, subkeys[round]);
  round++;
  sbox7(block, 0);
  BLOCK_XOR(block, subkeys[round]);
}

/* Apply an inverse Serpent round to BLOCK, using the SBOX number
   WHICH and the subkeys contained in SUBKEYS.  Use BLOCK_TMP as
   temporary storage.  This macro increments `round'.  */
#define IROUND_IMPL(which, block, subkey, round) \
  {                                              \
    LINEAR_TRANSFORMATION_INVERSE (block);       \
    ibox##which(block, 0);                       \
    BLOCK_XOR (block, subkey[round]);            \
  }

/* Apply the first Serpent round to BLOCK, using the SBOX number WHICH
   and the subkeys contained in SUBKEYS.  Use BLOCK_TMP as temporary
   storage.  The result will be stored in BLOCK_TMP.  This macro
   increments `round'.  */
static void first_iround(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round)
{
  BLOCK_XOR (block, subkeys[round]);
  round--;
  ibox7(block, 0);
  BLOCK_XOR (block, subkeys[round]);
}

static void round0(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) ROUND_IMPL(0, block, subkeys, round)
static void round1(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) ROUND_IMPL(1, block, subkeys, round)
static void round2(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) ROUND_IMPL(2, block, subkeys, round)
static void round3(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) ROUND_IMPL(3, block, subkeys, round)
static void round4(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) ROUND_IMPL(4, block, subkeys, round)
static void round5(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) ROUND_IMPL(5, block, subkeys, round)
static void round6(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) ROUND_IMPL(6, block, subkeys, round)
static void round7(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) ROUND_IMPL(7, block, subkeys, round)

static void iround0(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) IROUND_IMPL(0, block, subkeys, round)
static void iround1(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) IROUND_IMPL(1, block, subkeys, round)
static void iround2(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) IROUND_IMPL(2, block, subkeys, round)
static void iround3(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) IROUND_IMPL(3, block, subkeys, round)
static void iround4(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) IROUND_IMPL(4, block, subkeys, round)
static void iround5(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) IROUND_IMPL(5, block, subkeys, round)
static void iround6(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) IROUND_IMPL(6, block, subkeys, round)
static void iround7(uint32_t *block, serpent_subkeys_t subkeys, uint_fast8_t round) IROUND_IMPL(7, block, subkeys, round)

/* Convert the user provided key KEY of KEY_LENGTH bytes into the
   internally used format.  */
static void
serpent_key_prepare (const unsigned char *key, unsigned int key_length,
		     serpent_key_t key_prepared)
{
  int i;

  /* Copy key.  */
  for (i = 0; i < key_length / 4; i++)
    key_prepared[i] = stdc_load8_leu32 ((const unsigned char *)(((uint32_t *) key) + i));

  if (i < 8)
    {
      /* Key must be padded according to the Serpent
	 specification.  */
      key_prepared[i] = 0x00000001;

      for (i++; i < 8; i++)
	key_prepared[i] = 0;
    }
}

/* Derive the 33 subkeys from KEY and store them in SUBKEYS.  */
static void
serpent_subkeys_generate (serpent_key_t key, serpent_subkeys_t subkeys)
{
  uint32_t w_real[140];		/* The `prekey'.  */
  uint32_t *w = &w_real[8];
  int i, j;

  /* Initialize with key values.  */
  for (i = 0; i < 8; i++)
    w[i - 8] = key[i];

  /* Expand to intermediate key using the affine recurrence.  */
  for (i = 0; i < 132; i++)
    w[i] = stdc_rotate_left (w[i - 8] ^ w[i - 5] ^ w[i - 3] ^ w[i - 1] ^ PHI ^ i, 11);

  /* Calculate subkeys via S-Boxes, in bitslice mode.  */
  sbox3(w,   0);
  sbox2(w,   4);
  sbox1(w,   8);
  sbox0(w,  12);
  sbox7(w,  16);
  sbox6(w,  20);
  sbox5(w,  24);
  sbox4(w,  28);
  sbox3(w,  32);
  sbox2(w,  36);
  sbox1(w,  40);
  sbox0(w,  44);
  sbox7(w,  48);
  sbox6(w,  52);
  sbox5(w,  56);
  sbox4(w,  60);
  sbox3(w,  64);
  sbox2(w,  68);
  sbox1(w,  72);
  sbox0(w,  76);
  sbox7(w,  80);
  sbox6(w,  84);
  sbox5(w,  88);
  sbox4(w,  92);
  sbox3(w,  96);
  sbox2(w, 100);
  sbox1(w, 104);
  sbox0(w, 108);
  sbox7(w, 112);
  sbox6(w, 116);
  sbox5(w, 120);
  sbox4(w, 124);
  sbox3(w, 128);

  /* Renumber subkeys.  */
  for (i = 0; i < ROUNDS + 1; i++)
    for (j = 0; j < 4; j++)
      subkeys[i][j] = w[4 * i + j];
}

/* Initialize CONTEXT with the key KEY of KEY_LENGTH bits.  */
static void
serpent_setkey_internal (serpent_subkeys_t subkeys,
			 const unsigned char *key, unsigned int key_length)
{
  serpent_key_t key_prepared;

  serpent_key_prepare (key, key_length, key_prepared);
  serpent_subkeys_generate (key_prepared, subkeys);
}

#if defined (OP_encrypt)
void
serpent_encrypt_internal (serpent_subkeys_t subkeys,
			  const serpent_block_t input, serpent_block_t output)
{
  serpent_block_t b;
  int round = 0;

  b[0] = stdc_load8_leu32 ((const unsigned char *)(input + 0));
  b[1] = stdc_load8_leu32 ((const unsigned char *)(input + 1));
  b[2] = stdc_load8_leu32 ((const unsigned char *)(input + 2));
  b[3] = stdc_load8_leu32 ((const unsigned char *)(input + 3));

  round0(b, subkeys, round++);
  round1(b, subkeys, round++);
  round2(b, subkeys, round++);
  round3(b, subkeys, round++);
  round4(b, subkeys, round++);
  round5(b, subkeys, round++);
  round6(b, subkeys, round++);
  round7(b, subkeys, round++);
  round0(b, subkeys, round++);
  round1(b, subkeys, round++);
  round2(b, subkeys, round++);
  round3(b, subkeys, round++);
  round4(b, subkeys, round++);
  round5(b, subkeys, round++);
  round6(b, subkeys, round++);
  round7(b, subkeys, round++);
  round0(b, subkeys, round++);
  round1(b, subkeys, round++);
  round2(b, subkeys, round++);
  round3(b, subkeys, round++);
  round4(b, subkeys, round++);
  round5(b, subkeys, round++);
  round6(b, subkeys, round++);
  round7(b, subkeys, round++);
  round0(b, subkeys, round++);
  round1(b, subkeys, round++);
  round2(b, subkeys, round++);
  round3(b, subkeys, round++);
  round4(b, subkeys, round++);
  round5(b, subkeys, round++);
  round6(b, subkeys, round++);

  last_round(b, subkeys, round);

  output[0] = stdc_load8_leu32 ((const unsigned char *)(b + 0));
  output[1] = stdc_load8_leu32 ((const unsigned char *)(b + 1));
  output[2] = stdc_load8_leu32 ((const unsigned char *)(b + 2));
  output[3] = stdc_load8_leu32 ((const unsigned char *)(b + 3));
}
#endif

#if defined (OP_decrypt)
void
serpent_decrypt_internal (serpent_subkeys_t subkeys,
			  const serpent_block_t input, serpent_block_t output)
{
  serpent_block_t b;
  int round = ROUNDS;

  b[0] = stdc_load8_leu32 ((const unsigned char *)(input + 0));
  b[1] = stdc_load8_leu32 ((const unsigned char *)(input + 1));
  b[2] = stdc_load8_leu32 ((const unsigned char *)(input + 2));
  b[3] = stdc_load8_leu32 ((const unsigned char *)(input + 3));

  first_iround (b, subkeys, round); round -= 2;

  iround6 (b, subkeys, round--);
  iround5 (b, subkeys, round--);
  iround4 (b, subkeys, round--);
  iround3 (b, subkeys, round--);
  iround2 (b, subkeys, round--);
  iround1 (b, subkeys, round--);
  iround0 (b, subkeys, round--);
  iround7 (b, subkeys, round--);
  iround6 (b, subkeys, round--);
  iround5 (b, subkeys, round--);
  iround4 (b, subkeys, round--);
  iround3 (b, subkeys, round--);
  iround2 (b, subkeys, round--);
  iround1 (b, subkeys, round--);
  iround0 (b, subkeys, round--);
  iround7 (b, subkeys, round--);
  iround6 (b, subkeys, round--);
  iround5 (b, subkeys, round--);
  iround4 (b, subkeys, round--);
  iround3 (b, subkeys, round--);
  iround2 (b, subkeys, round--);
  iround1 (b, subkeys, round--);
  iround0 (b, subkeys, round--);
  iround7 (b, subkeys, round--);
  iround6 (b, subkeys, round--);
  iround5 (b, subkeys, round--);
  iround4 (b, subkeys, round--);
  iround3 (b, subkeys, round--);
  iround2 (b, subkeys, round--);
  iround1 (b, subkeys, round--);
  iround0 (b, subkeys, round--);

  output[0] = stdc_load8_leu32 ((const unsigned char *)(b + 0));
  output[1] = stdc_load8_leu32 ((const unsigned char *)(b + 1));
  output[2] = stdc_load8_leu32 ((const unsigned char *)(b + 2));
  output[3] = stdc_load8_leu32 ((const unsigned char *)(b + 3));
}
#endif

